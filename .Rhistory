geom_errorbar(aes(ymin = Morphine+Morphine.SEM, ymax = -Morphine.SEM), width = .2, color = "black", size = 1, position = position_dodge(.4)) +
scale_x_discrete(labels = c(
"Monkey cAMP" = "cAMP", "Monkey JNK" = "JNK",
"Monkey NFkB" = "NFkB"
)) +
theme(
panel.grid.major = element_blank(), panel.grid.minor = element_blank(),
panel.background = element_blank()
) + ggtitle("Morphine") +
xlab(NULL) + ylab("Δ logKa") + theme_pubclean()
ggsave("monkey_morphine.tiff", device = "tiff")
